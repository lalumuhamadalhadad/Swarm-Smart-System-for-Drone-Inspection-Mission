<?xml version="1.0"?>
<robot name="octagonal_building">

 <!-- Define the base link (octagonal floor) -->
 <link name="base_link">
   <inertial>
     <mass value="100.0"/>
     <origin xyz="0 0 0" rpy="0 0 0"/>
     <inertia ixx="100.0" ixy="0.0" ixz="0.0" iyy="100.0" iyz="0.0" izz="100.0"/>
   </inertial>
   <visual>
     <origin xyz="0 0 0" rpy="0 0 0"/>
     <geometry>
       <!-- Octagonal floor approximated using cylinder with 8 sides -->
       <cylinder radius="17.35" length="0.1"/>
     </geometry>
     <material name="floor_material">
       <color rgba="1 1 1 1"/>
     </material>
   </visual>
   <collision>
     <origin xyz="0 0 0" rpy="0 0 0"/>
     <geometry>
       <cylinder radius="17.35" length="0.1"/>
     </geometry>
   </collision>
 </link>

 <!-- Octagon Wall 1 (0 degrees - East) -->
 <link name="wall_1">
   <inertial>
     <mass value="5.0"/>
     <origin xyz="17.35 0 1.5" rpy="0 0 0"/>
     <inertia ixx="5.0" ixy="0.0" ixz="0.0" iyy="5.0" iyz="0.0" izz="5.0"/>
   </inertial>
   <visual>
     <origin xyz="17.35 0 1.5" rpy="0 0 0"/>
     <geometry>
       <box size="0.2 14.4 3"/>
     </geometry>
     <material name="wall_material">
       <color rgba="0.9 0.9 0.9 1"/>
     </material>
   </visual>
   <collision>
     <origin xyz="17.35 0 1.5" rpy="0 0 0"/>
     <geometry>
       <box size="0.2 14.4 3"/>
     </geometry>
   </collision>
 </link>

