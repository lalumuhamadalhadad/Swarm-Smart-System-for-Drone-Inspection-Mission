<?xml version="1.0"?>
<robot name="maze_layout">

  <!-- Define the base link (floor) -->
  <link name="base_link">
    <inertial>
      <mass value="1.0"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="30 30 0.1"/> <!-- Floor size -->
      </geometry>
      <material name="floor_color">
        <color rgba="0.8 0.8 0.8 1"/> <!-- Gray color -->
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="30 30 0.1"/>
      </geometry>
    </collision>
  </link>

  <!-- Define the outer walls -->
  <link name="outer_wall_1">
    <visual>
      <origin xyz="0 15 1.5" rpy="0 0 0"/>
      <geometry>
        <box size="30 1 3"/> <!-- Wall size -->
      </geometry>
      <material name="wall_color">
        <color rgba="0.9 0.9 0.9 1"/> <!-- Light gray color -->
      </material>
    </visual>
    <collision>
      <origin xyz="0 15 1.5" rpy="0 0 0"/>
      <geometry>
        <box size="30 1 3"/>
      </geometry>
    </collision>
  </link>

  <link name="outer_wall_2">
    <visual>
      <origin xyz="0 -15 1.5" rpy="0 0 0"/>
      <geometry>
        <box size="30 0.1 3"/> <!-- Wall size -->
      </geometry>
      <material name="wall_color"/>
    </visual>
    <collision>
      <origin xyz="0 -15 1.5" rpy="0 0 0"/>
      <geometry>
        <box size="30 0.1 3"/>
      </geometry>
    </collision>
  </link>

  <link name="outer_wall_3">
    <visual>
      <origin xyz="15 0 1.5" rpy="0 0 0"/>
      <geometry>
        <box size="1 30 3"/> <!-- Wall size -->
      </geometry>
      <material name="wall_color"/>
    </visual>
    <collision>
      <origin xyz="15 0 1.5" rpy="0 0 0"/>
      <geometry>
        <box size="1 30 3"/>
      </geometry>
    </collision>
  </link>

  <link name="outer_wall_4">
    <visual>
      <origin xyz="-15 0 1.5" rpy="0 0 0"/>
      <geometry>
        <box size="1 30 3"/> <!-- Wall size -->
      </geometry>
      <material name="wall_color"/>
    </visual>
    <collision>
      <origin xyz="-15 0 1.5" rpy="0 0 0"/>
      <geometry>
        <box size="1 30 3"/>
      </geometry>
    </collision>
  </link>

  <!-- Define inner walls for maze layout -->
  <!-- Horizontal walls -->
  <link name="inner_wall_1">
    <visual>
      <origin xyz="11.5 4.5 1.5" rpy="0 0 0"/>
      <geometry>
        <box size="8 0.5 3"/> <!-- Wall size -->
      </geometry>
      <material name="wall_color"/>
    </visual>
    <collision>
      <origin xyz="11.5 4.5 1.5" rpy="0 0 0"/>
      <geometry>
        <box size="8 0.5 3"/>
      </geometry>
    </collision>
  </link>

  <link name="inner_wall_2">
    <visual>
      <origin xyz="11.5 -4.5 1.5" rpy="0 0 0"/>
      <geometry>
        <box size="8 0.5 3"/> <!-- Wall size -->
      </geometry>
      <material name="wall_color"/>
    </visual>
    <collision>
      <origin xyz="11.5 -4.5 1.5" rpy="0 0 0"/>
      <geometry>
        <box size="8 0.5 3"/>
      </geometry>
    </collision>
  </link>

  <!-- Vertical walls -->
  <link name="inner_wall_3">
    <visual>
      <origin xyz="4.5 11.5 1.5" rpy="0 0 0"/>
      <geometry>
        <box size="0.5 8 3"/> <!-- Wall size -->
      </geometry>
      <material name="wall_color"/>
    </visual>
    <collision>
      <origin xyz="4.5 11.5 1.5" rpy="0 0 0"/>
      <geometry>
        <box size="0.5 8 3"/>
      </geometry>
    </collision>
  </link>

  <link name="inner_wall_4">
    <visual>
      <origin xyz="-4.5 11.5 1.5" rpy="0 0 0"/>
      <geometry>
        <box size="0.5 8 3"/> <!-- Wall size -->
      </geometry>
      <material name="wall_color"/>
    </visual>
    <collision>
      <origin xyz="-4.5 11.5 1.5" rpy="0 0 0"/>
      <geometry>
        <box size="0.5 8 3"/>
      </geometry>
    </collision>
  </link>

  <link name="inner_wall_5">
    <visual>
      <origin xyz="4.5 -11 1.5" rpy="0 0 0"/>
      <geometry>
        <box size="0.5 8 3"/> <!-- Wall size -->
      </geometry>
      <material name="wall_color"/>
    </visual>
    <collision>
      <origin xyz="4.5 -11 1.5" rpy="0 0 0"/>
      <geometry>
        <box size="0.5 8 3"/>
      </geometry>
    </collision>
  </link>

  <link name="inner_wall_6">
    <visual>
      <origin xyz="-4.5 -11 1.5" rpy="0 0 0"/>
      <geometry>
        <box size="0.5 8 3"/> <!-- Wall size -->
      </geometry>
      <material name="wall_color"/>
    </visual>
    <collision>
      <origin xyz="-4.5 -11 1.5" rpy="0 0 0"/>
      <geometry>
        <box size="0.5 8 3"/>
      </geometry>
    </collision>
  </link>

  <!-- Define the joints (fixed joints for walls) -->
  <joint name="base_to_outer_wall_1" type="fixed">
    <parent link="base_link"/>
    <child link="outer_wall_1"/>
  </joint>

  <joint name="base_to_outer_wall_2" type="fixed">
    <parent link="base_link"/>
    <child link="outer_wall_2"/>
  </joint>

  <joint name="base_to_outer_wall_3" type="fixed">
    <parent link="base_link"/>
    <child link="outer_wall_3"/>
  </joint>

  <joint name="base_to_outer_wall_4" type="fixed">
    <parent link="base_link"/>
    <child link="outer_wall_4"/>
  </joint>

  <joint name="base_to_inner_wall_1" type="fixed">
    <parent link="base_link"/>
    <child link="inner_wall_1"/>
  </joint>

  <joint name="base_to_inner_wall_2" type="fixed">
    <parent link="base_link"/>
    <child link="inner_wall_2"/>
  </joint>

  <joint name="base_to_inner_wall_3" type="fixed">
    <parent link="base_link"/>
    <child link="inner_wall_3"/>
  </joint>

  <joint name="base_to_inner_wall_4" type="fixed">
    <parent link="base_link"/>
    <child link="inner_wall_4"/>
  </joint>

  <joint name="base_to_inner_wall_5" type="fixed">
    <parent link="base_link"/>
    <child link="inner_wall_5"/>
  </joint>

  <joint name="base_to_inner_wall_6" type="fixed">
    <parent link="base_link"/>
    <child link="inner_wall_6"/>
  </joint>

</robot>