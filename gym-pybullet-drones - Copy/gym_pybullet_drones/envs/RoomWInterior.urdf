    </inertial>
    <visual>
      <origin xyz="0 7.5 1.5" rpy="0 0 0"/>
      <geometry>
        <box size="15 0.2 3"/>
      </geometry>
      <material name="wall_material">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 7.5 1.5" rpy="0 0 0"/>
      <geometry>
        <box size="15 0.2 3"/>
      </geometry>
    </collision>
  </link>

  <link name="south_wall">
    <inertial>
      <mass value="50.0"/>
      <origin xyz="0 -7.5 1.5" rpy="0 0 0"/>
      <inertia ixx="50.0" ixy="0.0" ixz="0.0" iyy="50.0" iyz="0.0" izz="50.0"/>
    </inertial>
    <visual>
      <origin xyz="0 -7.5 1.5" rpy="0 0 0"/>
      <geometry>
        <box size="15 0.2 3"/>
      </geometry>
      <material name="wall_material">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 -7.5 1.5" rpy="0 0 0"/>
      <geometry>
