<?xml version="1.0"?>
<robot name="indoor_environment">

  <!-- Floor -->
  <link name="base_link">
    <inertial>
      <mass value="1000.0"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1000.0" ixy="0.0" ixz="0.0" iyy="1000.0" iyz="0.0" izz="1000.0"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="15 15 0.1"/>
      </geometry>
      <material name="floor_material">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="15 15 0.1"/>
      </geometry>
    </collision>
  </link>

