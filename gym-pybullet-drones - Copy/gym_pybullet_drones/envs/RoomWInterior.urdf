    <visual>
      <origin xyz="-3.5 -3.5 0.4" rpy="0 0 0"/>
      <geometry>
        <box size="2.0 0.8 0.8"/> <!-- 3-seater sofa -->
      </geometry>
      <material name="leather_material">
        <color rgba="0.4 0.2 0.1 1"/> <!-- Brown leather -->
      </material>
    </visual>
    <collision>
      <origin xyz="-3.5 -3.5 0.4" rpy="0 0 0"/>
      <geometry>
        <box size="2.0 0.8 0.8"/>
      </geometry>
    </collision>
  </link>

