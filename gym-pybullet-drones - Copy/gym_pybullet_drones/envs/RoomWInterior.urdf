      <mass value="20.0"/>
      <origin xyz="0 0 0.8" rpy="0 0 0"/>
      <inertia ixx="20.0" ixy="0.0" ixz="0.0" iyy="20.0" iyz="0.0" izz="20.0"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0.8" rpy="0 0 0"/>
      <geometry>
        <box size="3.0 1.5 0.05"/> <!-- Large conference table -->
      </geometry>
      <material name="wood_material">
        <color rgba="0.6 0.4 0.2 1"/> <!-- Brown wood -->
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0.8" rpy="0 0 0"/>
      <geometry>
        <box size="3.0 1.5 0.05"/>
      </geometry>
    </collision>
  </link>

  <!-- Table Legs -->
  <link name="table_leg_1">
    <inertial>
      <mass value="2.0"/>
      <origin xyz="1.3 0.6 0.4" rpy="0 0 0"/>
      <inertia ixx="2.0" ixy="0.0" ixz="0.0" iyy="2.0" iyz="0.0" izz="2.0"/>
    </inertial>
    <visual>
      <origin xyz="1.3 0.6 0.4" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.05" length="0.8"/>
      </geometry>
      <material name="metal_material">
        <color rgba="0.7 0.7 0.7 1"/> <!-- Gray metal -->
      </material>
    </visual>
    <collision>
      <origin xyz="1.3 0.6 0.4" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.05" length="0.8"/>
      </geometry>
    </collision>
  </link>

  <link name="table_leg_2">
    <inertial>
      <mass value="2.0"/>
      <origin xyz="-1.3 0.6 0.4" rpy="0 0 0"/>
      <inertia ixx="2.0" ixy="0.0" ixz="0.0" iyy="2.0" iyz="0.0" izz="2.0"/>
    </inertial>
    <visual>
      <origin xyz="-1.3 0.6 0.4" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.05" length="0.8"/>
      </geometry>
      <material name="metal_material">
        <color rgba="0.7 0.7 0.7 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="-1.3 0.6 0.4" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.05" length="0.8"/>
      </geometry>
    </collision>
  </link>

  <link name="table_leg_3">
    <inertial>
      <mass value="2.0"/>
      <origin xyz="1.3 -0.6 0.4" rpy="0 0 0"/>
      <inertia ixx="2.0" ixy="0.0" ixz="0.0" iyy="2.0" iyz="0.0" izz="2.0"/>
    </inertial>
    <visual>
      <origin xyz="1.3 -0.6 0.4" rpy="0 0 0"/>
