      <geometry>
        <cylinder radius="0.05" length="0.8"/>
      </geometry>
    </collision>
  </link>

  <!-- Office Chairs around table -->
  <link name="chair_1">
    <inertial>
      <mass value="5.0"/>
      <origin xyz="0 2.5 0.5" rpy="0 0 0"/>
      <inertia ixx="5.0" ixy="0.0" ixz="0.0" iyy="5.0" iyz="0.0" izz="5.0"/>
    </inertial>
    <visual>
      <origin xyz="0 2.5 0.5" rpy="0 0 0"/>
      <geometry>
        <box size="0.5 0.5 1.0"/> <!-- Chair back and seat -->
      </geometry>
      <material name="fabric_material">
        <color rgba="0.2 0.2 0.8 1"/> <!-- Blue fabric -->
      </material>
    </visual>
    <collision>
      <origin xyz="0 2.5 0.5" rpy="0 0 0"/>
      <geometry>
        <box size="0.5 0.5 1.0"/>
      </geometry>
    </collision>
  </link>

  <link name="chair_2">
    <inertial>
      <mass value="5.0"/>
      <origin xyz="0 -2.5 0.5" rpy="0 0 0"/>
      <inertia ixx="5.0" ixy="0.0" ixz="0.0" iyy="5.0" iyz="0.0" izz="5.0"/>
    </inertial>
    <visual>
      <origin xyz="0 -2.5 0.5" rpy="0 0 0"/>
      <geometry>
        <box size="0.5 0.5 1.0"/>
      </geometry>
      <material name="fabric_material">
        <color rgba="0.2 0.2 0.8 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 -2.5 0.5" rpy="0 0 0"/>
      <geometry>
        <box size="0.5 0.5 1.0"/>
      </geometry>
    </collision>
  </link>



  <!-- Desk with Computer -->
  <link name="desk">
    <inertial>
      <mass value="15.0"/>
      <origin xyz="5.0 5.0 0.75" rpy="0 0 0"/>
      <inertia ixx="15.0" ixy="0.0" ixz="0.0" iyy="15.0" iyz="0.0" izz="15.0"/>
