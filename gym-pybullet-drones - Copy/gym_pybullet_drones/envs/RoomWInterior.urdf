      <mass value="50.0"/>
      <origin xyz="7.5 0 1.5" rpy="0 0 0"/>
      <inertia ixx="50.0" ixy="0.0" ixz="0.0" iyy="50.0" iyz="0.0" izz="50.0"/>
    </inertial>
    <visual>
      <origin xyz="7.5 0 1.5" rpy="0 0 0"/>
      <geometry>
        <box size="0.2 15 3"/>
      </geometry>
      <material name="wall_material">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="7.5 0 1.5" rpy="0 0 0"/>
      <geometry>
        <box size="0.2 15 3"/>
      </geometry>
    </collision>
  </link>

  <link name="west_wall">
    <inertial>
      <mass value="50.0"/>
      <origin xyz="-7.5 0 1.5" rpy="0 0 0"/>
