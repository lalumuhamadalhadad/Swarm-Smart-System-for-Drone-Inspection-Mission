      </geometry>
      <material name="wood_material">
        <color rgba="0.6 0.4 0.2 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="5.0 5.0 0.75" rpy="0 0 0"/>
      <geometry>
        <box size="1.5 0.8 0.05"/>
      </geometry>
    </collision>
  </link>

