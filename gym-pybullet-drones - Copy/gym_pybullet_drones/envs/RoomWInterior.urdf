  <link name="monitor">
    <inertial>
      <mass value="3.0"/>
      <origin xyz="5.0 5.3 1.1" rpy="0 0 0"/>
      <inertia ixx="3.0" ixy="0.0" ixz="0.0" iyy="3.0" iyz="0.0" izz="3.0"/>
    </inertial>
    <visual>
      <origin xyz="5.0 5.3 1.1" rpy="0 0 0"/>
      <geometry>
        <box size="0.6 0.05 0.4"/> <!-- Thin monitor -->
      </geometry>
      <material name="screen_material">
        <color rgba="0.1 0.1 0.1 1"/> <!-- Black screen -->
      </material>
    </visual>
    <collision>
      <origin xyz="5.0 5.3 1.1" rpy="0 0 0"/>
      <geometry>
        <box size="0.6 0.05 0.4"/>
      </geometry>
    </collision>
  </link>

  <!-- Bookshelf -->
  <link name="bookshelf">
    <inertial>
      <mass value="25.0"/>
      <origin xyz="-5.5 3.0 1.0" rpy="0 0 0"/>
      <inertia ixx="25.0" ixy="0.0" ixz="0.0" iyy="25.0" iyz="0.0" izz="25.0"/>
    </inertial>
    <visual>
      <origin xyz="-5.5 3.0 1.0" rpy="0 0 0"/>
      <geometry>
        <box size="0.3 2.0 2.0"/> <!-- Tall bookshelf -->
      </geometry>
      <material name="wood_material">
        <color rgba="0.5 0.3 0.1 1"/> <!-- Dark wood -->
      </material>
    </visual>
    <collision>
      <origin xyz="-5.5 3.0 1.0" rpy="0 0 0"/>
      <geometry>
        <box size="0.3 2.0 2.0"/>
      </geometry>
    </collision>
  </link>

  <!-- Whiteboard -->
  <link name="whiteboard">
    <inertial>
      <mass value="5.0"/>
      <origin xyz="0 7.3 1.5" rpy="0 0 0"/>
      <inertia ixx="5.0" ixy="0.0" ixz="0.0" iyy="5.0" iyz="0.0" izz="5.0"/>
    </inertial>
    <visual>
      <origin xyz="0 7.3 1.5" rpy="0 0 0"/>
      <geometry>
        <box size="2.0 0.05 1.0"/> <!-- Wall-mounted whiteboard -->
      </geometry>
      <material name="whiteboard_material">
        <color rgba="1 1 1 1"/> <!-- White surface -->
      </material>
    </visual>
    <collision>
      <origin xyz="0 7.3 1.5" rpy="0 0 0"/>
      <geometry>
        <box size="2.0 0.05 1.0"/>
      </geometry>
    </collision>
  </link>

  <!-- Coffee Table -->
  <link name="coffee_table">
    <inertial>
      <mass value="8.0"/>
      <origin xyz="3.5 -3.5 0.4" rpy="0 0 0"/>
      <inertia ixx="8.0" ixy="0.0" ixz="0.0" iyy="8.0" iyz="0.0" izz="8.0"/>
    </inertial>
    <visual>
      <origin xyz="3.5 -3.5 0.4" rpy="0 0 0"/>
      <geometry>
        <box size="1.0 0.6 0.05"/> <!-- Low coffee table -->
      </geometry>
      <material name="glass_material">
        <color rgba="0.9 0.9 1.0 0.7"/> <!-- Semi-transparent glass -->
      </material>
    </visual>
    <collision>
      <origin xyz="3.5 -3.5 0.4" rpy="0 0 0"/>
      <geometry>
        <box size="1.0 0.6 0.05"/>
      </geometry>
    </collision>
  </link>

  <!-- Sofa -->
  <link name="sofa">
    <inertial>
      <mass value="30.0"/>
      <origin xyz="-3.5 -3.5 0.4" rpy="0 0 0"/>
      <inertia ixx="30.0" ixy="0.0" ixz="0.0" iyy="30.0" iyz="0.0" izz="30.0"/>
