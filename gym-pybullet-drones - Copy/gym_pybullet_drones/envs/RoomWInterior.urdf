    <visual>
      <origin xyz="6.0 -5.0 0.6" rpy="0 0 0"/>
      <geometry>
        <box size="0.8 0.6 1.2"/> <!-- Printer stand with storage -->
      </geometry>
      <material name="metal_material">
        <color rgba="0.6 0.6 0.6 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="6.0 -5.0 0.6" rpy="0 0 0"/>
      <geometry>
        <box size="0.8 0.6 1.2"/>
      </geometry>
    </collision>
  </link>

  <!-- Ceiling Light Fixture -->
  <link name="ceiling_light">
    <inertial>
      <mass value="2.0"/>
      <origin xyz="0 0 2.8" rpy="0 0 0"/>
      <inertia ixx="2.0" ixy="0.0" ixz="0.0" iyy="2.0" iyz="0.0" izz="2.0"/>
    </inertial>
    <visual>
      <origin xyz="0 0 2.8" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.3" length="0.2"/>
      </geometry>
      <material name="light_material">
        <color rgba="1 1 0.8 1"/> <!-- Warm light color -->
      </material>
    </visual>
    <!-- No collision for ceiling light -->
  </link>

  <!-- Floor decorations/obstacles -->
  <link name="floor_plant">
    <inertial>
      <mass value="3.0"/>
      <origin xyz="2.5 6.0 0.7" rpy="0 0 0"/>
      <inertia ixx="3.0" ixy="0.0" ixz="0.0" iyy="3.0" iyz="0.0" izz="3.0"/>
    </inertial>
    <visual>
      <origin xyz="2.5 6.0 0.7" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.2" length="1.4"/> <!-- Tall plant pot -->
      </geometry>
      <material name="plant_material">
        <color rgba="0.1 0.6 0.1 1"/> <!-- Green plant -->
      </material>
    </visual>
    <collision>
      <origin xyz="2.5 6.0 0.7" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.2" length="1.4"/>
      </geometry>
    </collision>
  </link>

  <!-- ALL JOINTS (Fixed connections to base) -->
  <joint name="base_to_north_wall" type="fixed">
    <parent link="base_link"/>
    <child link="north_wall"/>
  </joint>

  <joint name="base_to_south_wall" type="fixed">
